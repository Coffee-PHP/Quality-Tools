{
    "name": "coffeephp/quality-tools",
    "description": "Quality tools for the CoffeePHP framework.",
    "license": "Apache-2.0",
    "version": "1.2.0",
    "type": "library",
    "readme": "README.md",
    "time": "2021-03-26",
    "authors": [
        {
            "name": "Danny Damsky",
            "email": "dannydamsky99@gmail.com"
        }
    ],
    "keywords": [
        "coffee",
        "php",
        "coffeephp",
        "quality",
        "tools",
        "dev",
        "static",
        "analysis",
        "test",
        "framework",
        "testing"
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "fakerphp/faker": "^1.13",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.6"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "CoffeePhp\\QualityTools\\": "src/"
        }
    },
    "bin": [
        "script/coffeephp_exec_phpunit",
        "script/coffeephp_exec_psalm",
        "script/coffeephp_exec_publish"
    ],
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "platform": {
            "php": "8.0"
        },
        "platform-check": false
    }
}
